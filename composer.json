{
    "name": "slothsoft/webrtc",
    "description": "A Farah module that implements the WebRTC spec.",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Daniel Schulz",
            "email": "info.slothsoft@gmail.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=7.0",
        "slothsoft/chat": "dev-master",
        "slothsoft/core": "dev-master",
        "slothsoft/farah": "dev-master",
        "slothsoft/sse": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5"
    },
    "autoload": {
        "psr-0": {
            "Slothsoft\\WebRTC\\": "src/"
        },
        "files": [
            "src/registerModule.php"
        ]
    },
    "keywords": [
        "farah-app",
        "farah-module",
        "slothsoft"
    ],
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "classmap-authoritative": false,
    "minimum-stability": "dev",
    "prefer-stable": true
}
